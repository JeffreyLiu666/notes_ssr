{
  "name": "notes-ssr",
  "version": "1.0.0",
  "description": "记录簿(node + vue-ssr)",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&& nodemon --watch src -e ts,tsx --exec ts-node ./src/main.ts",
    "open": "set NODE_ENV=development&& nodemon --watch src -e ts,tsx --exec ts-node ./src/main.ts --open",
    "build": "rimraf src/views/dist && npm run build:client && npm run build:server",
    "build:client": "set NODE_ENV=production&& webpack --mode production --config src/views/build/client.conf.js --progress --hide-modules",
    "build:server": "set NODE_ENV=production&& webpack --mode production --config src/views/build/server.conf.js --progress --hide-modules"
  },
